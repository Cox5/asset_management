<Window x:Class="AMS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AMS"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TabControl>
            <TabItem Header="Devices">
                <Label Content="Tabelarni prikaz svih uredjaja..." />
            </TabItem>
            <TabItem Header="Device Info">
                <Label Content="Informacije o uredjaju: sumirane promene i grafik za analogne tipove uredjaja"/>
            </TabItem>
            
        </TabControl>
    </Grid>
</Window>
